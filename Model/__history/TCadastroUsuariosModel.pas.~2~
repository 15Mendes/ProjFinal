unit TCadastroUsuariosModel;

interface

type
  TCUsuarios = class
  private
    FNome: string;
    FSenha: string;

  public
    function getNome: string;
    procedure setNome(const Value: string);
    function getSenha: string;
    procedure setSenha(const Value: string);
  end;

implementation

{ TUsuarios }

function TCUsuarios.getNome: string;
begin
  Result := FNome;
end;

procedure TCUsuarios.setNome(const Value: string);
begin
  FNome := Value;
end;

function TCUsuarios.getSenha: string;
begin
  Result := FSenha;
end;

procedure TCUsuarios.setSenha(const Value: string);
begin
  FSenha := Value;
end;

end.
